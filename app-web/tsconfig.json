{
	"extends": "@vue/tsconfig/tsconfig.json",
  "include": [
    "env.d.ts",
    "src/**/*",
    "src/**/*.vue"
  ],
  "exclude": [
    "node_modules",
    "./node_modules",
    "./node_modules/*"
  ],
  "compilerOptions": {
    "allowJs": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "baseUrl": ".",
    "verbatimModuleSyntax": true,
    "jsx": "preserve",
    "paths": {
      "@/*": [
        "./src/*"
      ]
    },
    "lib": [
      "es2015",
      "es2015.promise",
      "dom"
    ],
    "types": [
      "vite/client"
    ],
    "skipLibCheck": true,
    "preserveValueImports": false
  },
  "vueCompilerOptions": {
    "experimentalDisableTemplateSupport": false
  },
  "references": [
  ]
}
